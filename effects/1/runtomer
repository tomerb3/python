docker build -t lottie-overlay:latest .
docker run --rm -v "$PWD:/data" lottie-overlay:latest \
  node overlay.js \
  --video /data/333.mp4 \
  --lottie /data/Developer.json \
  --x 20 --y 22 --scale 4 \
  --start 2 --duration 18 --fps 30 \
  --overlayWidth 512 --overlayHeight 512 \
  --out /data/out.mp4
