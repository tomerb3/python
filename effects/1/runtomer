docker build -t lottie-overlay:latest .

cp -a /mnt/c/ffmpeg/before.mp4 .
cp -a /mnt/c/ffmpeg/circle.json .

docker run --rm -v "$PWD:/data" lottie-overlay:latest \
  node overlay.js \
  --video /data/before.mp4 \
  --lottie /data/circle.json \
  --x 20 --y 22 --scale 4 \
  --start 2 --duration 118 --fps 30 \
  --overlayWidth 512 --overlayHeight 512 \
  --out /data/before2.mp4
