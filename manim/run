docker run -it --rm \
  -v "$(pwd)":/manim \
  -u "$(id -u)":"$(id -g)" \
  manimcommunity/manim \
  manim -ql  ./text_scene.py TextScene

  a(){
    ffmpeg -i background.mp4 -i text_alpha.webm \
  -filter_complex "[1:v]scale=1920:-1[fg];[0:v][fg]overlay=0:0:format=auto" \
  -c:v libx264 -pix_fmt yuv420p -c:a copy output.mp4
  }